					$result .= '<tr valing="top">'."\n";
					$result .= '<td rowspan="'.count($question['answer']).'" width="20">'.($question['answer'][$_SESSION["TEST{$id}"][$question['id']]]['true']==1? "V": "X")."</td>\n";
					$result .= '<td rowspan="'.count($question['answer']).'">'.$question['name']."</td>\n";
